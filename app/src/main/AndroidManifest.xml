    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AuthorActivity"/>
        <activity android:name=".RestorationActivity"/>
        <activity android:name=".TechniqueActivity"/>
        <activity android:name=".PieceActivity"/>
        <activity android:name=".MuseumActivity"/>
        <activity android:name=".MovementActivity"/>
    </application>

</manifest>